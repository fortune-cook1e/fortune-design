{
  "name": "fortune-design",
  "version": "1.0.2",
  "description": "A react ui component library",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "scripts": {
    "dev": "pnpm -C docs install && pnpm -C docs dev",
    "build": "pnpm run /^build:.*/",
    "build:gulp": "gulp build --gulpfile scripts/gulpFile.js",
    "build:webpack": "webpack --mode production",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly --outDir lib/cjs && tsc --project tsconfig.build.json --emitDeclarationOnly --outDir lib/esm",
    "lint": "pnpm run /^lint:.*/",
    "lint:style": "stylelint src/**/*.less",
    "lint:tsx": "eslint src --ext .js,.jsx,.ts,.tsx --cache --fix",
    "clear": "rimraf lib",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "README.md",
    "lib"
  ],
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "pnpm lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fortune-cook1e/fortune-design.git"
  },
  "keywords": [
    "react",
    "component"
  ],
  "author": "fortune_cookie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fortune-cook1e/fortune-design/issues"
  },
  "homepage": "https://github.com/fortune-cook1e/fortune-design#readme",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "cssnano": "^6.0.0",
    "eslint": "^8.38.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.22",
    "postcss-less": "^6.0.0",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.2",
    "stylelint": "^15.5.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1"
  },
  "sideEffects": [
    "cjs/**/styles/*",
    "esm/**/styles/*",
    "dist/*",
    "*.less",
    "*.css"
  ]
}
